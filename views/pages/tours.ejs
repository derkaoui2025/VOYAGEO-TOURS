                        Explore Our Tours
                    </h2>
                </div>
                
                <div class="mt-6 md:mt-0" data-aos="fade-left">
                    <button id="toggle-filters" class="flex items-center text-secondary hover:text-primary transition-colors">
                        <i class="fas fa-sliders-h mr-2"></i> 
                        <span>Advanced Filters</span>
                        <i class="fas fa-chevron-down ml-2 transition-transform duration-300"></i>
                    </button>
                </div>
            </div>
            
            <!-- Quick Filters -->
            <div class="flex flex-wrap gap-3 mt-8" data-aos="fade-up">
                <div class="filter-pill active" data-filter="all">All Tours</div>
                
                <% 
                // Get unique destinations from tours
                const destinations = [...new Set(tours.map(tour => tour.startLocation))];
                const categories = [...new Set(tours.map(tour => tour.category))];
                
                // Output destination tags
                destinations.forEach(destination => { %>
                    <div class="filter-pill" data-filter="<%= destination.toLowerCase().replace(/\s+/g, '-') %>">
                       <%= destination %>
                    </div>
                <% });
                
                // Output category tags
                categories.forEach(category => { %>
                    <div class="filter-pill" data-filter="<%= category.toLowerCase() %>">
                       <%= category %>
                    </div>
                <% }); %>
            </div>
    
            <!-- Advanced Filters (Hidden by Default) -->
            <div id="advanced-filters" class="mt-8 bg-beige p-6 hidden" data-aos="fade-up">
                <div class="grid grid-cols-1 md:grid-cols-2 lg:grid-cols-4 gap-6">
                    <div class="space-y-2">
                        <label class="text-secondary font-medium text-sm flex items-center">
                            <i class="fas fa-clock text-primary mr-2"></i> Duration
                            </label>
                        <select id="duration-filter" class="w-full px-4 py-3 border border-stone/30 focus:border-primary transition-colors bg-white">
                                <option value="">All Durations</option>
                                <option value="1-3">1-3 Days</option>
                                <option value="4-7">4-7 Days</option>
                                <option value="8-14">8-14 Days</option>
                                <option value="15+">15+ Days</option>
                            </select>
                        </div>
                        
                    <div class="space-y-2">
                        <label class="text-secondary font-medium text-sm flex items-center">
                            <i class="fas fa-map-marker-alt text-primary mr-2"></i> Destination
                            </label>
                        <select id="destination-filter" class="w-full px-4 py-3 border border-stone/30 focus:border-primary transition-colors bg-white">
                                <option value="">All Destinations</option>
                                <% destinations.forEach(destination => { %>
                                <option value="<%= destination %>"><%= destination %></option>
                                <% }); %>
                            </select>
                        </div>
                        
                    <div class="space-y-2">
                        <label class="text-secondary font-medium text-sm flex items-center">
                            <i class="fas fa-tag text-primary mr-2"></i> Price Range
                            </label>
                        <select id="price-filter" class="w-full px-4 py-3 border border-stone/30 focus:border-primary transition-colors bg-white">
                                <option value="">All Prices</option>
                                <option value="0-500">€0 - €500</option>
                                <option value="500-1000">€500 - €1000</option>
                                <option value="1000-2000">€1000 - €2000</option>
