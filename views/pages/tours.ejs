                        stone: '#A49E8D',
                        charcoal: '#1F1F1F'
                    },
                    fontFamily: {
                        serif: ['Cormorant Garamond', 'serif'],
                        sans: ['Urbanist', 'sans-serif']
                    },
                    fontSize: {
                        '4.5xl': '2.5rem',
                        '5.5xl': '3.5rem',
                        '6.5xl': '4rem'
