    <!-- AOS Animation Library -->
    <script src="https://unpkg.com/aos@next/dist/aos.js"></script>
    <script>
        document.addEventListener('DOMContentLoaded', function() {
            // Initialize AOS animations
            AOS.init({
                duration: 800,
                once: true,
                offset: 100,
                easing: 'ease-out-cubic'
            });
            
            // Toggle advanced filters
            const toggleFiltersBtn = document.getElementById('toggle-filters');
            const advancedFilters = document.getElementById('advanced-filters');
            
            toggleFiltersBtn.addEventListener('click', function() {
                advancedFilters.classList.toggle('hidden');
                const icon = this.querySelector('.fa-chevron-down');
                icon.classList.toggle('rotate-180');
            });
            
            // Quick filter functionality
            const filterPills = document.querySelectorAll('.filter-pill');
            const tourCards = document.querySelectorAll('.tour-card');
            
            filterPills.forEach(pill => {
                pill.addEventListener('click', function() {
                    // Remove active class from all pills
                    filterPills.forEach(p => p.classList.remove('active'));
                    
                    // Add active class to clicked pill
                    this.classList.add('active');
                    
                    const filter = this.getAttribute('data-filter');
                    
                    // Show/hide tours based on filter
                    tourCards.forEach(card => {
                        if (filter === 'all') {
                            card.style.display = 'block';
                        } else {
                            if (card.getAttribute('data-destination') === filter || 
                                card.getAttribute('data-category') === filter) {
                                card.style.display = 'block';
                            } else {
                                card.style.display = 'none';
                            }
                        }
                    });
                });
            });
            
            // Advanced filter functionality
            const applyFiltersBtn = document.getElementById('apply-filters');
            const durationFilter = document.getElementById('duration-filter');
            const destinationFilter = document.getElementById('destination-filter');
            const priceFilter = document.getElementById('price-filter');
            const categoryFilter = document.getElementById('category-filter');
            
            applyFiltersBtn.addEventListener('click', function() {
                const durationValue = durationFilter.value;
                const destinationValue = destinationFilter.value;
                const priceValue = priceFilter.value;
                const categoryValue = categoryFilter.value;
                
                tourCards.forEach(card => {
                    let showCard = true;
                    
                    // Duration filter
                    if (durationValue && durationValue !== '') {
                        const tourDuration = parseInt(card.getAttribute('data-duration'));
                        
                        if (durationValue === '1-3') {
                            showCard = tourDuration >= 1 && tourDuration <= 3;
                        } else if (durationValue === '4-7') {
                            showCard = tourDuration >= 4 && tourDuration <= 7;
                        } else if (durationValue === '8-14') {
                            showCard = tourDuration >= 8 && tourDuration <= 14;
                        } else if (durationValue === '15+') {
                            showCard = tourDuration >= 15;
                    }
                    
                        if (!showCard) {
                            card.style.display = 'none';
                            return;
                        }
                    }
                    
                    // Destination filter
                    if (destinationValue && destinationValue !== '') {
                        const tourDestination = card.getAttribute('data-destination');
                        if (destinationValue.toLowerCase().replace(/\s+/g, '-') !== tourDestination) {
                            card.style.display = 'none';
                            return;
                    }
                    }
                    
                    // Price filter
                    if (priceValue && priceValue !== '') {
                        const tourPrice = parseInt(card.getAttribute('data-price').replace(/[^0-9]/g, ''));
                        
                        if (priceValue === '0-500') {
                            showCard = tourPrice >= 0 && tourPrice <= 500;
                        } else if (priceValue === '500-1000') {
                            showCard = tourPrice > 500 && tourPrice <= 1000;
                        } else if (priceValue === '1000-2000') {
                            showCard = tourPrice > 1000 && tourPrice <= 2000;
                        } else if (priceValue === '2000+') {
                            showCard = tourPrice > 2000;
                    }
                    
                        if (!showCard) {
                            card.style.display = 'none';
                            return;
                        }
                    }
                    
                    // Category filter
                    if (categoryValue && categoryValue !== '') {
                        const tourCategory = card.getAttribute('data-category');
                        if (categoryValue.toLowerCase() !== tourCategory) {
                            card.style.display = 'none';
                            return;
                    }
                    }
                    
                    // If all filters pass, show the card
                    card.style.display = 'block';
                });
            });
        });
    </script>
</body>
