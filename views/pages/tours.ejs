                        <label class="text-secondary font-medium text-sm flex items-center">
                            <i class="fas fa-th-large text-primary mr-2"></i> Category
                            </label>
                        <select id="category-filter" class="w-full px-4 py-3 border border-stone/30 focus:border-primary transition-colors bg-white">
                                <option value="">All Categories</option>
                                <% categories.forEach(category => { %>
                                    <option value="<%= category %>"><%= category %></option>
                                <% }); %>
                            </select>
                        </div>
                    </div>
                    
                <div class="flex justify-center mt-6">
                    <button id="apply-filters" class="btn-primary">
                        <i class="fas fa-filter mr-2"></i> Apply Filters
